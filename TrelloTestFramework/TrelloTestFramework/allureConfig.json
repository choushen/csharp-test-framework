            "severity": "^(normal|blocker|critical|minor|trivial)$"
        },
        "links": {
            "link": "^allure\\.link:(.+)$",
            "issue": "^allure\\.issue:(.+)$",
            "tms": "^allure\\.tms:(.+)$"
        }
    }
